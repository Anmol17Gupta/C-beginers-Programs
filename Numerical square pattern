#include <stdio.h>

int main(){
    int n=7;
    
    for(int i=1;i<=n;i++){
        for(int j=i;j<=n;j++){  //for upperhalf triangle
            printf("%d",j);
        }
        for(int k=1;k<=i-1;k++){  //for lowerhalf triangle
            printf("%d",k);
        }
        printf("\n");
    }
    
    return 0;
}
