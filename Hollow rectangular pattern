#include <stdio.h>
int main(){
    int l;  //length=4
    int b;  //breadth=6
    scanf("%d",&l);
    scanf("%d",&b);
    
    for(int i=1;i<=l;i++){
        for(int j=1;j<=b;j++){
            if(i==1 || i==l || j==1 || j==b){
                printf("*");}
            else{
                printf(" ");
            }
        }printf("\n");
    }
    
    return 0;
}
